{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
      //"Grpc": "Trace"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Http": {
        "Url": "https://*:7071"
      }
    },
    "Certificates": {
      "Default": {
        "Subject": "your_certif_full_subject_name",
        "Store": "My",
        "Location": "LocalMachine",
        "AllowInvalid": true
      }
    }
  },
  "StoredFilesPath": "./uploads"
}
